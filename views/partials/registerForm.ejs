<div class="row">
    <div class="col-md-6">
        <h3 class="text-primary form-header">Informácie o členovi</h3>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="firstName">Meno*</label>
                <input type="text" class="form-control" id="firstName" placeholder="Krstné meno" name="firstName"
                    value="<%= typeof firstName != 'undefined' ? firstName : '' %>" required
                    pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z\u0100-\u017Fáéíýóúô]*)$">
            </div>
            <div class="form-group col-md-12">
                <label for="lastName">Priezvisko*</label>
                <input type="text" class="form-control" id="lastName" placeholder="Priezvisko" name="lastName"
                    value="<%= typeof lastName != 'undefined' ? lastName : '' %>" required
                    pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z\u0100-\u017Fáéíýóúô]*)$">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="email">Email*</label>
                <input type="email" class="form-control" id="email" placeholder="Email" name="email"
                    value="<%= typeof email != 'undefined' ? email : '' %>" required>
            </div>
            <div class="form-group col-md-12">
                <label for="phoneNum">Tel. číslo</label>
                <input type="text" class="form-control" id="phoneNum" placeholder="Telefónne číslo" name="phoneNum"
                    value="<%= typeof phoneNum != 'undefined' ? phoneNum : '' %>" pattern="^[+]?[()/0-9. -]{9,}$">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12" id="dob">
                <label for="dob" style="padding-left: 5px;">Dátum narodenia*</label>
                <input type="date" class="form-control" id="dob" placeholder="Deň" name="dob"
                    value="<%= typeof dob != 'undefined' ? dob : '' %>" required>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12" id="dob">
                <label for="address">Adresa*</label>
                <input type="text" class="form-control" id="address" placeholder="napr. Tatranská 8" name="address"
                    value="<%= typeof address != 'undefined' ? address : '' %>" required
                    pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z0-9\u0100-\u017F áéíýóúô]*)$">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="city">Mesto*</label>
                <input type="text" class="form-control" id="city" name="city"
                    value="<%= typeof city != 'undefined' ? city : '' %>" placeholder="napr. Banská Bystrica" required
                    pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z\u0100-\u017F áéíýóúô]*)$">
            </div>
            <div class="form-group col-md-12">
                <label for="state">Štát*</label>
                <input type="text" id="state" class="form-control" name="state"
                    value="<%= typeof state != 'undefined' ? state : '' %>" required
                    pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z\u0100-\u017F áéíýóúô]*)$">
            </div>
            <div class="form-group col-md-12">
                <label for="PSC">PSČ*</label>
                <input type="text" class="form-control" id="PSC" name="PSC"
                    value="<%= typeof PSC != 'undefined' ? PSC : '' %>" required pattern="^[0-9\s]*$">
            </div>

        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="inf">Zdravotné oslabenia, alergie a choroby na ktoré člen trpí / lieky, ktoré užíva:
                </label>
                <textarea name="inf" class="form-control" rows="2"
                    id="inf"><%= typeof inf != 'undefined' ? inf : '' %></textarea>
            </div>
        </div>

    </div>
    <div class="col-md-6">
        <h3 class="text-primary form-header">Informácie o zákonnom zástupcovi</h3>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="parent-firstName">Meno*</label>
                <input type="text" class="form-control" id="parent-firstName" placeholder="Krstné meno"
                    name="parentFirstName" value="<%= typeof parentFirstName != 'undefined' ? parentFirstName : '' %>"
                    required pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z\u0100-\u017Fáéíýóúô]*)$">
            </div>
            <div class="form-group col-md-12">
                <label for="parentLastName">Priezvisko*</label>
                <input type="text" class="form-control" id="parentLastName" placeholder="Priezvisko"
                    name="parentLastName" value="<%= typeof parentLastName != 'undefined' ? parentLastName : '' %>"
                    required pattern="^(?:[A-zÀ-ú]+)(?:[A-Za-z\u0100-\u017Fáéíýóúô]*)$">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="parentEmail">Email*</label>
                <input type="parentEmail" class="form-control" id="parentEmail" placeholder="Email" name="parentEmail"
                    value="<%= typeof parentEmail != 'undefined' ? parentEmail : '' %>" required>
            </div>
            <div class="form-group col-md-12">
                <label for="parentPhoneNum">Tel. číslo</label>
                <input type="text" class="form-control" id="parentPhoneNum" placeholder="Telefónne číslo"
                    name="parentPhoneNum" value="<%= typeof parentPhoneNum != 'undefined' ? parentPhoneNum : '' %>"
                    pattern="^[+]?[()/0-9. -]{9,}$">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-12">
                <label for="parentInf">Poznámka:
                </label>
                <textarea name="parentInf" class="form-control" rows="2"
                    id="parentInf"><%= typeof parentInf != 'undefined' ? parentInf : '' %></textarea>
            </div>
        </div>

        <div class="form-row">
            <div class="form-check form-group col-md-12" style="margin-bottom: 0">
                <input class="form-check-input" type="checkbox" id="gridCheck" name="checkbox" required>
                <label class="form-check-label" for="gridCheck">
                    Súhlasím s <a href="/obchodne-podmienky">obchodnými podmienkami</a>*
                </label>
            </div>
        </div>
        <div class="form-row">
            <div class="form-check form-group col-md-12">
                <input class="form-check-input" type="checkbox" id="GDPRCheck" name="checkboxGDPR" required>
                <label class="form-check-label" for="GDPRCheck">
                    Súhlasím so spracovaním <a href="/osobne-udaje">osobných údajov</a>*
                </label>
            </div>
        </div>
        <button type="submit"
            <%= typeof user != 'undefined' && 'data-toggle="modal" data-target="#exampleModalCenter"' %>
            class="btn btn-primary ml-5">Odoslať</button>
    </div>
</div>